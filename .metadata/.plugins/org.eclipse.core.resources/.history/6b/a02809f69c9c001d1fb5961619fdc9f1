package com.UserAPI;

import java.util.Scanner;

public class User {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String cases;
		System.out.println("1.LinkedList\n2.Queue\n3.Stack\n\nSelect Operation: ");
		int op1 = sc.nextInt();
//		
		switch (op1) {
		case 1:
			cases = "LinkedList";
			break;
		case 2:
			cases = "Queue";
			break;
		case 3:
			cases = "Stack";
			break;
		default:
			cases = "Option Not Found!";
		}
		
		if(cases.equals("LinkedList")) {
			System.out.print("1.insert()\ninsertAt()\ninsertStart()\ndeleteAt()\ndeleteAll()\nshow()\n\nSelect Operation: ");
			int op2 = sc.nextInt();
			switch(op2) {
			case 1:
				
			}
		}
		
	}
}
