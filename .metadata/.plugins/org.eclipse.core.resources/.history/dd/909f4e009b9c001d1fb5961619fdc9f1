package com.UserAPI;

import java.util.Scanner;

public class User {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String cases;
		System.out.println("1.LinkedList\n2.Queue\n3.Stack\n");

		System.out.print("Select Operation: ");
		int input = sc.nextInt();
//		
		switch (input) {
		case 1:
			cases = "LinkedList";
			break;
		case 2:
			cases = "Queue";
			break;
		case 3:
			cases = "Stack";
			break;
		default:
			cases = "Option Not Found!";
		}
		
		if(cases.equals("LinkedList")) {
			
		}
		
	}
}
