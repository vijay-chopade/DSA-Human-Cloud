package com.stack;

public class Stack {
	
	private int size = 10;
	int stack[] = new int[this.size];
	private int position = 0;
	
	public boolean isEmpty() {
		
	}
	public void push(int data) {
		stack[position] = data;
		position++;
	}

	public String pop() {
		String pop;
		position--;
		stack[position] = 0;
		pop = "" + stack[position];
		return pop;
	}

	public int  fatch() {
		return stack[position - 1];
	}

	public void show() {
		for (int i = 0; i < position; i++) {
			System.out.print(stack[i] + " ");
		}
		System.out.println();
	}

	public int length() {
		return position;
	}

	public boolean pick(int data) {
		boolean check = false;
		for (int s : stack) {
			if (s == data) {
				check = true;
			}
		}
		return check;
	}

}
