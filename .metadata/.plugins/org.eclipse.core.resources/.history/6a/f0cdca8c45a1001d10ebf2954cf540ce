package com.ServerAPI;

import java.util.ArrayList;

public class Menu extends MenuItem {
	private int index;
	private String ds;
	private ArrayList<MenuItem> menuItem = new ArrayList<>();

	public Menu() {
	}

	public Menu(int index, String ds) {
		super();
		this.index = index;
		this.ds = ds;
	}

	public Menu(int index, String ds, MenuItem menuItem) {
		super();
		this.index = index;
		this.ds = ds;
		this.menuItem = menuItem;
	}

	public int getIndex() {
		return index;
	}

	public void setIndex(int index) {
		this.index = index;
	}

	public String getDs() {
		return ds;
	}

	public void setDs(String ds) {
		this.ds = ds;
	}

	public Menu getMenu() {
		return menu;
	}

	@Override
	public String toString() {
		return "Menu [index=" + index + ", ds=" + ds + "]";
	}

}
