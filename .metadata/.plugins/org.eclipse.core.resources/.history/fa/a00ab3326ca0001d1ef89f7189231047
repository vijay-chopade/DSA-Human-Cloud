package com.Array;

public class Runner {

	public static void main(String[] args) {
//		Array arr = new Array();
		
//		int [] c = new int[6];
		Array [] arr = new Array[3];
		arr[0] = new Array(1);
		arr[1] = new Array(2);
		arr[2] = new Array(3);
//		arr[3] = new Array(4);
		
		System.out.println(arr.length);
