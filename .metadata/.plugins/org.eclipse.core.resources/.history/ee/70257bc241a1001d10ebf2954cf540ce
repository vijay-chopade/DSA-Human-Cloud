package com.ServerAPI;

public class Menu {
	int index;
	String ds;

//	Menu menu;
	public Menu() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Menu(int index, String ds) {
		super();
		this.index = index;
		this.ds = ds;
	}


	public int getIndex() {
		return index;
	}

	public void setIndex(int index) {
		this.index = index;
	}

	public String getDs() {
		return ds;
	}

	public void setDs(String ds) {
		this.ds = ds;
	}

	@Override
	public String toString() {
		return "Menu [index=" + index + ", ds=" + ds + "]";
	}

}
